{
  "manifest_version": 2,
  "name": "Phones",
  "version": "3.0.0",
  "icons": {
    "16": "images/phone_16.png",
    "32": "images/phone_32.png",
    "64": "images/phone_64.png"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "/js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/",
        "http://*/"
      ],
      "js": [
        "/js/end.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/phone_16.png",
      "32": "images/phone_32.png",
      "64": "images/phone_64.png"
    },
    "default_title": "Справочник тел. АПИ [ver 3.0]",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "notifications"
  ]
}