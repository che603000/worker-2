{
  "manifest_version": 2,
  "name": "Phones",
  "version": "3.0.0",
  "background": {
    "persistent": true,
    "scripts": [
      "/js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/",
        "http://*/"
      ],
      "js": [
        "/js/end.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "Справочник тел. АПИ",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "notifications"
  ]
}